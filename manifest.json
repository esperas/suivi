{
    "_version": "0.1.2",
    "sap.app": {
        "_version": "0.1.2",
        "id": "ecole.famille",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "0.1.2"
        }
    },
    "sap.ui": {
        "_version": "0.1.1",
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_bluecrystal"
        ]
    },
    "sap.ui5": {
        "_version": "1.30",
        "rootView": "ecole.famille.view.App",
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ecole.famille.i18n.i18n"
                }
            },
            "famille": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/data.json"
            },
            "menu": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/Menu.json"
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "ecole.famille.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                        "target": "404"
                }
            },
            "routes": [{
                "pattern": "",
                "name": "init",
                "target": "home"
            }],
            "targets": {
                "home": {
                    "viewName": "Split",
                    "viewLevel" : 1
                },
                "404": {
                    "viewName": "404",
                    "transition": "show"
                },
                "nologin": {
                    "viewName": "NoLogin",
                    "transition": "show"
                },
                "menu": {
                    "viewName": "Menu",
                    "transition": "show"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        }
    }
}
