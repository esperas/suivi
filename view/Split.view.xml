<mvc:View controllerName="ecole.famille.controller.Split"
          xmlns:l="sap.ui.layout"
          xmlns:mvc="sap.ui.core.mvc"
          xmlns="sap.m"
          displayBlock="true">


    <Page title="Association Los Amics de la Calandreta de la Dauna">
        <customHeader>
		  <Bar>
		      <contentLeft>
		          <!-- Rien pour -->
		      </contentLeft>

		      <contentMiddle>
                  <Text text = "Association Los Amics de la Calandreta de la Dauna"></Text>
		      </contentMiddle>

              <contentRight>
                  <Button id="help" icon="sap-icon://sys-help" press="onHelp" ariaLabelledBy="editButtonLabel"/>
              </contentRight>
            </Bar>
		</customHeader>
				<SplitContainer
						id="Split"
						initialDetail="suivi"
						initialMaster="menu"
						mode="PopoverMode">
					<detailPages >
						<mvc:XMLView id="suivi" viewName="ecole.famille.view.Suivi" />
                        <mvc:XMLView id="famille" viewName="ecole.famille.view.Famille" />
                        <mvc:XMLView id="apropos" viewName="ecole.famille.view.Apropos" />
					</detailPages>
					<masterPages>
						<mvc:XMLView id="menu" viewName="ecole.famille.view.Menu" />
					</masterPages>
				</SplitContainer>
			</Page>




</mvc:View>
